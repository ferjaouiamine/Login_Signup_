<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".FirebaseAuthenticationApp"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyApplication"
        tools:targetApi="31">
        <activity
            android:name=".sampledata.SampleData"
            android:exported="false"
            android:label="@string/title_activity_sample_data"
            android:theme="@style/Theme.MyApplication" />
        <activity
            android:name=".Tp32Activity"
            android:exported="false"
            android:label="@string/title_activity_tp32"
            android:theme="@style/Theme.MyApplication" />
        <activity
            android:name=".Tp31Activity"
            android:exported="false"
            android:label="@string/title_activity_tp31"
            android:theme="@style/Theme.MyApplication" />
        <activity
            android:name=".Tp2Activity"
            android:exported="false"
            android:label="@string/title_activity_tp2"
            android:theme="@style/Theme.MyApplication" />
        <activity
            android:name=".Tp1Activity"
            android:exported="false"
            android:label="@string/title_activity_tp1"
            android:theme="@style/Theme.MyApplication" />
        <activity
            android:name=".MenuActivity"
            android:exported="false"
            android:label="@string/title_activity_menu"
            android:theme="@style/Theme.MyApplication" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.MyApplication"
            android:windowSoftInputMode="adjustResize">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>